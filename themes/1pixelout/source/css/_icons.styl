@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?-402v9j');
	src:url('../fonts/icomoon.eot?-402v9j#iefix') format('embedded-opentype'),
		url('../fonts/icomoon.ttf?-402v9j') format('truetype'),
		url('../fonts/icomoon.woff?-402v9j') format('woff'),
		url('../fonts/icomoon.svg?-402v9j#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

